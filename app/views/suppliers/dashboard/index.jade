extends ../../layouts/base

block content
   link(rel="stylesheet", href="/css/suppliers/dashboard.css")
   
   include header
   - if (typeof userFirst !== 'undefined' && userFirst)
        include intro
   
   - else if (typeof store !== 'undefined' && !userFirst)
        - if (typeof products !== 'undefined')
            include store-details
        - else 
            include product-first

   - else 
        include error



     